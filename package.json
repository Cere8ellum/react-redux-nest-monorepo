{
  "name": "react-redux-nest-monorepo",
  "version": "1.0.0",
  "description": "Monorepo to start fullstack dev using React and Nest",
  "homepage": "https://github.com/Cere8ellum/react-redux-nest-monorepo",
  "author": "Cere8ellum",
  "config": {
    "web_dev": "--config apps/web/__config__/webpack/dev.config.js",
    "web_prod": "--config apps/web/__config__/webpack/prod.config.js",
    "api_dev": "--config apps/api/__config__/webpack/dev.config.js",
    "api_prod": "--config apps/api/__config__/webpack/prod.config.js"
  },
  "scripts": {
    "start:dev": "concurrently --kill-others \"npm run start-api:dev\" \"npm run start-web:dev \"",
    "start:prod": "concurrently --kill-others \"npm run start-api:prod\" \"npm run start-web:prod \"",
    "start-web:dev": "cross-env-shell webpack serve ${npm_package_config_web_dev}",
    "start-web:prod": "cross-env-shell webpack serve ${npm_package_config_web_prod}",
    "start-api:dev": "cross-env-shell webpack --watch ${npm_package_config_api_dev}",
    "start-api:prod": "cross-env-shell webpack --watch ${npm_package_config_api_prod}",
    "build-web:dev": "cross-env-shell webpack ${npm_package_config_web_dev}",
    "build-web:prod": "cross-env-shell webpack ${npm_package_config_web_prod}",
    "build-api:dev": "cross-env-shell webpack ${npm_package_config_api_dev}",
    "build-api:prod": "cross-env-shell webpack ${npm_package_config_api_prod}",
    "clean": "rd /s /q build",
    "lint": "eslint \"**/*.{ts,tsx}\"",
    "lint-fix": "npm run lint --fix",
    "test-web": "cross-env jest --config apps/web/__config__/jest/jest.config.js",
    "test-web-watch": "jest --watch --config apps/web/__config__/jest/jest.config.js",
    "test-web-coverage": "jest --coverage --config apps/web/__config__/jest/jest.config.js",
    "test-api": "cross-env jest --config apps/api/__config__/jest/jest.config.js",
    "test-api-watch": "jest --watch --config apps/api/__config__/jest/jest.config.js",
    "test-api-coverage": "jest --coverage --config apps/api/__config__/jest/jest.config.js"
  },
  "keywords": [],
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/swagger": "^7.1.8",
    "@nestjs/testing": "^10.0.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/react": "^19.0.8",
    "@types/supertest": "^2.0.12",
    "@types/webpack-env": "^1.18.1",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "babel-jest": "^29.6.1",
    "babel-loader": "^9.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.33.1",
    "favicons": "^7.2.0",
    "favicons-webpack-plugin": "^6.0.0",
    "filemanager-webpack-plugin": "^8.0.0",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.3",
    "image-minimizer-webpack-plugin": "^3.8.3",
    "imagemin": "^8.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^10.0.1",
    "jest": "^29.5.0",
    "jest-css-modules-transform": "^4.4.2",
    "jest-environment-jsdom": "^29.6.2",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss-loader": "^7.3.3",
    "postcss-preset-env": "^9.0.0",
    "react-refresh": "^0.14.0",
    "run-script-webpack-plugin": "^0.2.0",
    "sass-loader": "^13.3.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6",
    "typescript-plugin-css-modules": "^5.0.1",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.13.7",
    "@mui/material": "^5.13.7",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@reduxjs/toolkit": "^2.5.1",
    "axios": "^1.5.1",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.14.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.5"
  }
}
